                "filters": [
                  {
                    "clusterOut": "$",
                    "classOut": "0",
                    "minParticleSize": -1,
                    "maxParticleSize": -1,
                    "minCircularity": 0,
                    "snapAreaSize": 0
                  }
                ]
              }
            ]
          }
        },
        {
          "$measure": {
            "clustersIn": [
              "$"
            ],
            "classesIn": [
              "0"
            ],
            "planesIn": [
              {
                "imageIdx": "MaxIntensity",
                "imagePlane": {
                  "cStack": 3
                }
              }
            ]
          }
        },
        {
          "$imageFromClass": {
            "classesIn": [
              "0"
            ]
          }
        },
        {
          "$saveImage": {
            "compression": 0,
            "namePrefix": "_classified_A",
            "objectCluster": "$"
          }
        }
      ]
    },
