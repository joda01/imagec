                    }
                },
                {
                    "$classify": {
                        "objectClasses": [
                            {
                                "classId": "CL_00",
                                "grayscaleValue": 65535,
                                "filter": {
                                    "minParticleSize": 0,
                                    "maxParticleSize": -1,
                                    "minCircularity": 0,
                                    "snapAreaSize": 0
