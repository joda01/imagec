{
    "images": {
        "zStackHandling": "IntensityProjection",
        "tStackHandling": "ExactOne",
        "imageInputDirectory": "test/test_data/images",
        "resultsOutputFolder": "tmp"
    },
    "pipelines": [
        {
            "channelLoader": {
                "imageChannelIndex": "CH_03",
                "zProjection": "MaxIntensity"
            },
            "pipelineSteps": [
                {
                    "$blur": {
                        "kernelSize": 6,
                        "repeat": 5
                    }
                },
                {
                    "$saveImage": {
                        "compression": 0
                    }
                },
                {
                    "$threshold": {
                        "node": "Manual",
                        "thresholdMin": 10,
                        "thresholdMax": 65535
                    }
                },
                {
                    "$watershed": {}
                },
                {
                    "$saveImage": {
                        "compression": 0,
                        "namePrefix": "_thresholded"
                    }
                },
                {
                    "$classify": {
                        "objectClasses": [
                            {
                                "classId": "CL_00",
                                "grayscaleValue": 65535,
                                "filter": {
                                    "minParticleSize": 0,
                                    "maxParticleSize": -1,
                                    "minCircularity": 0,
                                    "snapAreaSize": 0
                                }
                            }
                        ]
                    }
                },
                {
                    "$imageFromClass": {
                        "objectClasses": [
                            "CL_00"
                        ]
                    }
                },
                {
                    "$saveImage": {
                        "compression": 0,
                        "namePrefix": "_classified"
                    }
                }
            ]
        },
        {
            "channelLoader": {
                "imageChannelIndex": "CH_04",
                "zProjection": "MaxIntensity"
            },
            "pipelineSteps": [
                {
                    "$blur": {
                        "kernelSize": 3,
                        "repeat": 1
                    }
                },
                {
                    "$saveImage": {
                        "compression": 0
                    }
                }
            ]
        }
    ]
}
