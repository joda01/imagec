{
    "imageLoader": {
        "zStackHandling": "EachIndividual",
        "tStackHandling": "ExactOne",
        "imageInputDirectory": "test/test_data/images",
        "resultsOutputFolder": "tmp"
    },
    "pipelines": [
        {
            "channelLoader": {
                "imageChannelIndex": "CH_03",
                "zProjection": "MaxIntensity"
            },
            "pipelineSteps": [
                {
                    "$blur": {
                        "kernelSize": 6,
                        "repeat": 5
                    }
                },
                {
                    "$saveImage": {
                        "compression": 0
                    }
                },
                {
                    "$threshold": {
                        "node": "Manual",
                        "thresholdMin": 10,
                        "thresholdMax": 65535
                    }
                },
                {
                    "$watershed": {}
                },
                {
                    "$saveImage": {
                        "compression": 0,
                        "namePrefix": "_thresholded"
                    }
                }
            ]
        },
        {
            "channelLoader": {
                "imageChannelIndex": "CH_04",
                "zProjection": "MaxIntensity"
            },
            "pipelineSteps": [
                {
                    "$blur": {
                        "kernelSize": 3,
                        "repeat": 1
                    }
                },
                {
                    "$saveImage": {
                        "compression": 0
                    }
                }
            ]
        }
    ]
}
