                                "0": {
                                    "slotID": "$1",
                                    "$classId": "4",
                                    "name": "My name"
                                },
                                "1": {
                                    "slotID": "$2",
                                    "$classId": "6",
                                    "name": "My name"
                                }
                            }
                        }
                    }
                },
                {
                    "$input": "$1",
                    "$bitwiseOr": {
                        "slots": [
                            "$2"
                        ]
                    }
                },
                {
                    "$input": "$",
                    "$watershed": {}
                },
                {
                    "$input": "$",
                    "$store": {
                        "slotID": "$6",
                        "name": "My name"
                    }
                },
                {
                    "$input": "$",
                    "$writeControlImage": {
                        "name": "my_name"
                    }
                },
                {
                    "$filter": {
                        "minSize": 10,
                        "maxSize": 100,
                        "minCircularity": 50,
                        "$referenceSpotRemoval": {
                            "referenceSpots": "$5"
                        }
                    }
                },
                {
                    "$input": "$",
                    "$measure": {
                        "meta": {
                            "name": "Name of my measurement",
                            "classId": "$classId",
                            "channel": "$channel",
                            "zStack": "$zStack",
                            "tStack": "$tStack"
                        }
                    }
                }
            ]
        }
