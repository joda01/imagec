				@{->CentrePane()@}#ifequal $default_pane "1"
				@{->DefaultPane()@}#ifequal $toolbar_pane "1"
				@{->ToolbarPane()@}; #nl
				@$this->m_mgr->AddPane( @$this->$name, new wxAuiPaneInfo() );
			@}
			@}
