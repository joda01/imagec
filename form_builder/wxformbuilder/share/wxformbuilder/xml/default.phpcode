		<!-- wxMoveEvent -->
		<template name="evt_connect_OnMove">@$this->$name->Connect( wxEVT_MOVE, #handler );</template>

		<!-- wxUpdateUIEvent -->
		<template name="evt_connect_OnUpdateUI">@$this->$name->Connect( wxEVT_UPDATE_UI, #handler );</template>
